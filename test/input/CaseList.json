            "preview": {
              "name": "shape",
              "value": {
                "image": {
                  "name": "string",
                  "required": true
                },
                "type": {
                  "name": "enum",
                  "value": [
                    {
                      "value": "'iphone'",
                      "computed": false
                    },
                    {
                      "value": "'ipad'",
                      "computed": false
                    },
                    {
                      "value": "'desktop'",
                      "computed": false
                    },
                    {
                      "value": "'dual'",
                      "computed": false
                    }
                  ],
                  "required": true
                }
              },
              "required": false
            }
          }
        }
      },
      "required": true,
      "description": ""
