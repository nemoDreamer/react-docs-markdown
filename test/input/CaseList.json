    "items": {
      "type": {
        "name": "arrayOf",
        "value": {
          "name": "shape",
          "value": {
            "className": {
              "name": "string",
              "required": false
            },
            "backgroundColor": {
              "name": "string",
              "required": false
            },
            "backgroundImage": {
              "name": "string",
              "required": false
            },
            "color": {
              "name": "string",
              "required": false
            },
            "fullWidth": {
              "name": "bool",
              "required": false
            },
            "category": {
              "name": "string",
              "required": false
            },
            "title": {
              "name": "string",
              "required": true
            },
            "body": {
              "name": "string",
              "required": false
            },
            "link": {
              "name": "shape",
              "value": {
                "href": {
                  "name": "string",
                  "required": true
                },
                "target": {
                  "name": "string",
                  "required": false
                }
              },
              "required": true
