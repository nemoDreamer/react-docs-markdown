    "margin": {
      "type": {
        "name": "union",
        "value": [
          {
            "name": "shape",
            "value": {
              "top": {
                "name": "enum",
                "value": [
                  {
                    "value": "'none'",
                    "computed": false
                  },
                  {
                    "value": "'small'",
                    "computed": false
                  },
                  {
                    "value": "'large'",
                    "computed": false
                  }
                ],
                "required": false
              },
              "bottom": {
                "name": "enum",
                "value": [
                  {
                    "value": "'none'",
                    "computed": false
                  },
                  {
                    "value": "'small'",
                    "computed": false
                  },
                  {
                    "value": "'large'",
                    "computed": false
                  }
                ],
                "required": false
              }
            }
          },
          {
            "name": "enum",
            "value": [
              {
                "value": "'none'",
                "computed": false
              },
              {
                "value": "'small'",
                "computed": false
              },
              {
                "value": "'large'",
                "computed": false
              }
            ]
          }
        ]
      },
      "required": false,
      "description": "Margin can be either a string, or an object with top/bottom margin",
      "defaultValue": {
        "value": "'small'",
        "computed": false
      }
    },
    "component": {
      "type": {
        "name": "node"
      },
      "required": false,
      "description": "@internal Use another root component."
    },
    "backgroundColor": {
      "type": {
        "name": "string"
      },
      "required": true,
      "description": "@custom-exclude"
    }
